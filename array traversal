#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int n;
    cout << "Enter the size of the array: ";
    cin >> n; 
    
    vector<int> arr(n);
    
    cout << "Enter the elements of the array: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i]; 
    }
    
    cout << "The elements you entered are: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " "; 
    }

    return 0;
}
